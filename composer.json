{
    "name": "vincentbean/laravel-plausible",
    "description": "Laravel Plausible",
    "keywords": [
        "VincentBean",
        "laravel",
        "laravel-plausible"
    ],
    "homepage": "https://github.com/vincentbean/laravel-plausible",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Bean",
            "email": "info@vincentbean.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^8.37|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "VincentBean\\LaravelPlausible\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VincentBean\\LaravelPlausible\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VincentBean\\LaravelPlausible\\LaravelPlausibleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.20|^7.0|^8.0"
    }
}
